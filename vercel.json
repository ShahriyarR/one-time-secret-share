{
    "builds": [{
        "src": "src/onetime/entrypoints/web/app/wsgi.py/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/onetime/entrypoints/web/app/wsgi.py"
        }
    ]
}